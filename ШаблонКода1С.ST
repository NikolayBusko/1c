	ДанныеКонтрагента.Вставить(""РегистрационныйНомер"",	ДанныеЕГРЮЛ[""ogrn""]);
	ДанныеКонтрагента.Вставить(""ДатаРегистрации"",		ДанныеЕГРЮЛ[""reg_date""]);
